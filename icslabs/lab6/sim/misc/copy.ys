# Name: Songtao Xue ID: 520030910034   

.pos 0
		irmovq stack, %rsp
		call main
		halt

.align 8
# Source block
src:
		.quad 0x00a
		.quad 0x0b0
		.quad 0xc00

# Destination block
dest:
		.quad 0x111
		.quad 0x222
		.quad 0x333

# copy_block((int *src, int *dest, int len)
copy_block:
		pushq %r12      
		pushq %r13      
		pushq %r14		
		irmovq $1, %r13
		irmovq $8, %r14
		irmovq $0, %rax 
test:
        andq %rdx, %rdx
        jle END
loop:
		mrmovq 0(%rdi), %r12  
		addq %r14, %rdi       
		rmmovq %r12, (%rsi)   
		addq %r14, %rsi       
		xorq %r12, %rax
		subq %r13, %rdx
        jmp test
END:
		popq %r14
		popq %r13
		popq %r12
		ret

main:
		irmovq src, %rdi
		irmovq dest, %rsi
		irmovq $3, %rdx
		call copy_block
		ret

.pos 0x300
stack:
